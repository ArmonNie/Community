<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean">
	<class name="UserUpload" table="userupload">
		<id name="uploadid" column="uploadid" type="integer">
			<generator class="native"/>
		</id>
		<property name="filenumber" column="filenumber" type="string"></property>
		<property name="usernumber" column="usernumber" type="string"></property>
		<property name="uploadnumber" column="uploadnumber" type="string"></property>
	</class>
</hibernate-mapping>