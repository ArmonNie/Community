<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean">
	<class name="File" table="file">
		<id name="fileid" column="fileid" type="integer">
			<generator class="native"/>
		</id>
		<property name="filenumber" column="filenumber" type="string"></property>
		<property name="filename" column="filename" type="string"></property>
		<property name="filepath" column="filepath" type="string"></property>
		<property name="filesize" column="filesize" type="integer"></property>
		<property name="goodnumber" column="goodnumber" type="integer"></property>
	</class>
</hibernate-mapping>